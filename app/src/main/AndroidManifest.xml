<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sptech.rxjavabysp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".rxJava2Examples.CompositeObservableActivity"></activity>
        <activity android:name=".rxJava2Examples.DisposableObserverActivity" />
        <activity android:name=".rxJava2Examples.DisposableActivity" />
        <activity android:name=".rxJava2Examples.SimpleObserverActivity" />
        <activity android:name=".rxJava2Examples.RxJava2Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>